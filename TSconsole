#<main + arguments + section>#
if [[ ${filter} == 'TSconsole -u' || ${filter} == 'TSconsole --update' ]]; then
INTERNET
git pull
elif [[ ${filter} == 'TSconsole -i' || ${filter} == 'TSconsole --install' ]]; then
bash assets/programs/setup.TSconsole
elif [[ ${filter} == 'TSconsole -r' || ${filter} == 'TSconsole --remove' ]]; then
bash assets/programs/remove.TSconsole
#======args for bruteforce prograns======#
elif [[ $1 == '--brute' || $1 == '-b' ]]; then
  if [[ $2 == 'social-media' ]]; then
   bash assets/load.sh "" "               " 1
   echo
   socialBrute
  elif [[ $2 == 'hashes' ]]; then
   bash assets/load.sh "" "               " 1
   echo
   hashBrute
  elif [[ $2 == 'help' || $2 == '?' || $2 == 'h' ]]; then
   bruteHelp
  else
   play assets/sound/erro.mp3 trim 0 1 > /dev/null 2>&1 | printf "${S2}[${S1}!${S2}]${S7}Unsatisfied argument ‘${S4}${2}’${S7} :: execute ‘TSconsole -b ?’ or ‘TSconsole --brute help’${R0}\n"
  fi
#---x---#
#=======args for encoder programs========#
elif [[ $1 == '--encode' || $1 == '-e' ]]; then
  if [[ $2 == 'hashes' ]];  then
    bash assets/load.sh "" "              " 1
    echo
    hashEncode
  elif [[ $2 == 'binary' ]]; then
    bash assets/load.sh "" "              " 1
    echo
    binaryEncode
  elif [[ $2 == 'help' || $2 == '?' || $2 == 'h' ]]; then
   bash assets/load.sh "" "               " 1
   encodeHelp
  else
    play assets/sound/erro.mp3 trim 0 1 > /dev/null 2>&1 | printf "${S2}[${S1}!${S2}]${S7}Unsatisfied argument ‘${S4}${2}’${S7} :: execute ‘TSconsole -e ?’ or ‘TSconsole --encode help’${R0}\n"
  fi
#---x---#
#+++++++args for decoder programs++++++++#
elif [[ $1 == '--decode' || $1 == '-d' ]]; then
  if [[ $2 == 'binary' ]];  then
    bash assets/load.sh "" "              " 1
    echo
    binaryDecode
  elif [[ $2 == 'help' || $2 == '?' || $2 == 'h' ]]; then
   bash assets/load.sh "" "                " 1
   decodeHelp
  else
    play assets/sound/erro.mp3 trim 0 1 > /dev/null 2>&1 | printf "${S2}[${S1}!${S2}]${S7}Unsatisfied argument ‘${S4}${2}’${S7} :: execute ‘TSconsole -d ?’ or ‘TSconsole --decode help’${R0}\n"
  fi
#---x---#
#++++++++++args for phish programs+++++++++#
elif [[ $1 == '--phish' || $1 == '-p' ]]; then
  if [[ $2 == 'camera' ]];  then
    bash assets/load.sh "" "              " 1
    echo
    cameraPhish
  elif [[ $2 == 'website' || $2 == 'webpage' ]]; then
    bash assets/load.sh "" "             " 1
    echo
    webPhish
  elif [[ $2 == 'help' || $2 == '?' || $2 == 'h' ]]; then
   phishHelp
  else
    play assets/sound/erro.mp3 trim 0 1 > /dev/null 2>&1 | printf "${S2}[${S1}!${S2}]${S7}Unsatisfied argument ‘${S4}${2}’${S7} :: execute ‘TSconsole -p ?’ or ‘TSconsole --phish help’${R0}\n"
  fi
#---x---#
#++++++++++args for spam programs+++++++++++#
elif [[ $1 == '--spam' || $1 == '-s' ]]; then
  if [[ $2 == 'call-sms' ]];  then
    bash assets/load.sh "" "              " 1
    echo
    bombSpam
  elif [[ $2 == 'help' || $2 == '?' || $2 == 'h' ]]; then
